        sandboxId
        updatedDate
      }
      pcReleaseDate
      prePurchase
      approximateReleasePlan {
        day
        month
        quarter
        year
        releaseDateType
      }
      price(country: $country) {
        totalPrice {
          discountPrice
          originalPrice
          voucherDiscount
          discount
          currencyCode
          currencyInfo {
            decimals
          }
          fmtPrice(locale: $locale) {
            originalPrice
            discountPrice
            intermediatePrice
          }
        }
        lineOffers {
          appliedRules {
            id
            endDate
            discountSetting {
              discountType
            }
          }
        }
      }
      allDependNsOfferIds
      majorNsOffers {
        categories {
          path
        }
        id
        namespace
        title
      }
      subNsOffers {
        categories {
          path
        }
        id
        namespace
        price(country: $country) {
          totalPrice {
            discountPrice
